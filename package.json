{
  "name": "pretty-list-routes",
  "version": "0.0.4",
  "description": "ğŸ›£ï¸ A beautiful `list:routes` for AdonisJS",
  "main": "index.js",
  "files": [
    "build/commands"
  ],
  "scripts": {
    "pretest": "npm run lint",
    "test": "node .bin/test.js",
    "lint": "eslint . --ext=.ts",
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "clean": "del-cli build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "format": "prettier --write .",
    "release": "np --message=\"ğŸ”– bumped to %s\"",
    "version": "npm run build"
  },
  "author": "Julien Ripouteau <julien@ripouteau.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julien-R44/pretty-list-routes.git"
  },
  "keywords": [
    "adonisjs",
    "adonis",
    "ace",
    "cli",
    "list:routes"
  ],
  "bugs": {
    "url": "https://github.com/Julien-R44/pretty-list-routes/issues"
  },
  "homepage": "https://github.com/Julien-R44/pretty-list-routes#readme",
  "license": "MIT",
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptPackage",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "devDependencies": {
    "@adonisjs/core": "^5.4.2",
    "@adonisjs/mrm-preset": "^5.0.2",
    "@adonisjs/require-ts": "^2.0.8",
    "@types/node": "^17.0.8",
    "del-cli": "^4.0.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "japa": "^4.0.0",
    "mrm": "^3.0.10",
    "np": "^7.6.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "adonisjs": {
    "commands": [
      "pretty-list-routes/build/commands/PrettyRoute.js"
    ]
  }
}
